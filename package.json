{
  "name": "screeps",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm run watch:private",
    "lint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "build": "rollup -c",
    "push:main": "rollup -c --environment DEST:main",
    "push:private": "rollup -c --environment DEST:private",
    "push:sim": "rollup -c --environment DEST:sim",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:main": "rollup -cw --environment DEST:main",
    "watch:private": "rollup -cw --environment DEST:private",
    "watch:sim": "rollup -cw --environment DEST:sim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jomik/screeps.git"
  },
  "keywords": [
    "screeps",
    "ai",
    "typescript"
  ],
  "author": "Jonas Damtoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jomik/screeps/issues"
  },
  "homepage": "https://github.com/Jomik/screeps#readme",
  "devDependencies": {
    "@types/lodash": "^3.10.1",
    "@types/node": "^10.5.5",
    "@types/screeps": "^2.4.0",
    "prettier": "^1.14.0",
    "rollup": "^0.64.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "source-map": "^0.6.1"
  },
  "prettier": {
    "arrowParens": "always"
  }
}
